{
    "name": "pokemon-code",
    "displayName": "pokemon-code",
    "description": "Catch Pokemon in augmented code-reality",
    "version": "0.0.5",
    "publisher": "mattkremer",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.startPokemonCode"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.startPokemonCode",
            "title": "Pokemon Code - Start"
        },{
            "command": "extension.showPokemon",
            "title": "Pokemon Code - Pokedex"
        },{
            "command": "extension.showInventory",
            "title": "Pokemon Code - Inventory"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}